{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "docs/**",
    "node_modules/**",
    "**/*.d.ts",
    "test/**/*.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "public/*",
    "dist/*",
    "**/*.js",
    "copyStaticAssets.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}